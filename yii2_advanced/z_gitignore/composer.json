{
	"name": "p2made/yii2-p2made",
	"description": "Pedro's making stuff website",
	"type": "project",
	"license": "proprietary",
	"minimum-stability": "dev",
	"prefer-stable": true,

	"require": {
		"php": ">=5.6.0",
		"yiisoft/yii2": "~2.0.14",
		"yiisoft/yii2-swiftmailer": "~2.0.0 || ~2.1.0",
		"p2made/yii2-a6e": "~4.0",
		"p2made/yii2-p2y2-kartik-collection": "~0.1",

		"rmrevin/yii2-fontawesome": "~3.0",
		"amnah/yii2-user": "~5.0",
		"borales/yii2-phone-input": "~0.1.2",
		"cebe/yii2-gravatar": "~1.1",
		"dektrium/yii2-user": "~0.9.14",
		"kartik-v/yii2-datecontrol": "~1.9",
		"kartik-v/yii2-dialog": "~1.0",
		"kartik-v/yii2-field-range": "~1.3",
		"kartik-v/yii2-social": "~1.3",
		"kartik-v/yii2-widgets": "~3.4"
	},
	"require-dev": {
		"yiisoft/yii2-debug": "~2.1.0",
		"yiisoft/yii2-gii": "~2.1.0",
		"yiisoft/yii2-faker": "~2.0.0",
		"codeception/codeception": "^4.0",
		"codeception/module-asserts": "^1.0",
		"codeception/module-yii2": "^1.0",
		"codeception/module-filesystem": "^1.0",
		"codeception/verify": "~0.5.0 || ~1.1.0",
		"symfony/browser-kit": ">=2.7 <=4.2.4",

		"p2made/yii2-p2y2-gii-collection": "dev-master"
	},
	"config": {
		"process-timeout": 1800,
		"fxp-asset": {
			"enabled": false
		}
	},
	"repositories": [
		{
			"type": "composer",
			"url": "https://asset-packagist.org"
		}
	]
}
