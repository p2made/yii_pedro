    "type": "project",
    "license": "BSD-3-Clause",
    "minimum-stability": "stable",
    "require": {
        "php": ">=5.4.0",
        "yiisoft/yii2": "*",
        "yiisoft/yii2-bootstrap": "*",
        "yiisoft/yii2-swiftmailer": "*",
        "p2made/yii2-p2y2-things": ">=1.2.2",
        "p2made/yii2-p2y2-things-demo": ">=1.2.2",
        "kartik-v/yii2-widgets": "*",
        "kartik-v/yii2-date-range": "*",
        "kartik-v/yii2-datecontrol": "*",
        "kartik-v/yii2-password": "*",
        "kartik-v/yii2-detail-view": "*",
        "kartik-v/yii2-dynagrid": "*",
        "kartik-v/yii2-editable": "*",
        "kartik-v/yii2-icons": "*",
        "kartik-v/yii2-checkbox-x": "*",
        "kartik-v/yii2-context-menu": "*",
        "kartik-v/yii2-dropdown-x": "*",
        "kartik-v/yii2-export": "*",
        "kartik-v/yii2-field-range": "*",
        "kartik-v/yii2-label-inplace": "*",
        "kartik-v/yii2-money": "*",
        "kartik-v/yii2-nav-x": "*",
        "kartik-v/yii2-slider": "*",
        "kartik-v/yii2-social": "*",
        "kartik-v/yii2-sortable-input": "*",
        "kartik-v/yii2-tabs-x": "*",
        "guzzlehttp/guzzle": "*",
        "amnah/yii2-user": "dev-master"
    },
    "require-dev": {
        "yiisoft/yii2-codeception": "*",
        "yiisoft/yii2-debug": "*",
        "yiisoft/yii2-gii": "*",
        "yiisoft/yii2-faker": "*"
    },
    "config": {
        "process-timeout": 1800
    },
    "extra": {
        "asset-installer-paths": {
            "npm-asset-library": "vendor/npm",
            "bower-asset-library": "vendor/bower"
        }
